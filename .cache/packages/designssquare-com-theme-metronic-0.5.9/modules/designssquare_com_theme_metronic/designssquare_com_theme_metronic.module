}

function designssquare_com_theme_metronic_ckeditor_wrap_alter(&$content_wrap, $vars, $region)
{
    $prefix = '';
    $postfix = '';

    switch ($region) {
        case 'header':
            $prefix = '<div class="page-header navbar navbar-fixed-top">';
            $prefix .= '    <div class="page-header-inner">';
            $prefix .= '        <div class="top-menu">';
            $prefix .= '            <ul class="nav navbar-nav pull-right ckeditor-wrapper-end">';
            $postfix = '            </ul>';
            $postfix .= '       </div>';
            $postfix .= '   </div>';
            $postfix .= '</div>';
            break;
        case 'footer':
            $prefix = '<div class="page-footer">';
            $prefix .= '     <div class="page-footer-inner ckeditor-wrapper-end">';
            $postfix = '     </div>';
            $postfix .= '</div>';
            break;
        case 'login_footer':
            $prefix = '<div class="page-footer">';
            $prefix .= '     <div class="page-footer-inner ckeditor-wrapper-end">';
            $postfix = '     </div>';
            $postfix .= '</div>';
            break;
        case 'theme_panel':
            $prefix = '<div class="page-container">';
            $prefix .= '    <div class="page-content-wrapper">';
            $prefix .= '        <div class="page-content">';
            $prefix .= '            <div class="theme-panel hidden-xs hidden-sm ckeditor-wrapper-end" >';
            $postfix = '            </div>';
            $postfix .= '       </div>';
            $postfix .= '   </div>';
            $postfix .= '</div>';
            break;
        case 'highlighted':
            $prefix = '<div class="page-container">';
            $prefix .= '     <div class="page-content-wrapper">';
            $prefix .= '         <div class="page-content">';
            $prefix .= '                <div id="highlighted ckeditor-wrapper-end" >';
            $postfix = '                </div>';
            $postfix .= '        </div>';
            $postfix .= '    </div>';
            $postfix .= '</div>';
            break;
        case 'breadcrumbs_sidebar':
            $prefix = '<div class="page-container">';
            $prefix .= '    <div class="page-content-wrapper">';
            $prefix .= '        <div class="page-content">';
            $prefix .= '            <div class="row">';
            $prefix .= '                <div class="col-md-12">';
            $prefix .= '                    <ul class="page-breadcrumb breadcrumb ckeditor-wrapper-end" >';
            $postfix = '                    </ul>';
            $postfix .= '               </div>';
            $postfix .= '           </div>';
            $postfix .= '       </div>';
            $postfix .= '   </div>';
            $postfix .= '</div>';
            break;
        case 'sidebar':
            $prefix = '<div class="page-container"><div class="page-sidebar-wrapper"><div class="page-sidebar navbar-collapse collapse" ><ul class="page-sidebar-menu" data-auto-scroll="true" data-slide-speed="200"><li class="sidebar-toggler-wrapper"><div class="sidebar-toggler"></div></li></ul><span class="ckeditor-wrapper-end">';
            $postfix = '</span></div></div></div>';
            break;
        case 'quick_sidebar':
            $prefix = '<div class="page-header navbar navbar-fixed-top">';
            $prefix .= '   <div class="page-header-inner">';
            $prefix .= '        <div class="top-menu">';
            $prefix .= '            <ul class="nav navbar-nav pull-right">';
            $prefix .= '                    <li class="dropdown dropdown-quick-sidebar-toggler">';
            $prefix .= '                           <a href="javascript:;" class="dropdown-toggle">';
            $prefix .= '                                <i class="icon-logout"></i>';
            $prefix .= '                           </a>';
            $prefix .= '                    </li>';
            $prefix .= '            </ul>';
            $prefix .= '        </div>';
            $prefix .= '    </div>';
            $prefix .= '</div>';
            $prefix .= '<div class="page-container ">';
            $prefix .= '   	<a href="javascript:;" class="page-quick-sidebar-toggler"><i class="icon-close"></i></a>';
            $prefix .= '    <div class="page-quick-sidebar-wrapper ckeditor-wrapper-end">';
            $postfix = '    </div>';
            $postfix .= '</div>';
            break;
        case 'main': //main content
            $prefix = '<div class="page-container"><div class="page-content-wrapper"><div class="page-content ckeditor-wrapper-end">';
            $postfix = '</div></div></div>';
            break;
    }
    $content_wrap['prefix'] = $prefix;
    $content_wrap['postfix'] = $postfix;
}

function designssquare_com_theme_metronic_ckeditor_js_scripts_alter(&$js_scripts, $vars)
{
    $type = (isset($vars['node']) && isset($vars['node']->type)) ? $vars['node']->type : 'default';

     base_path() . $path_to_theme = drupal_get_path('theme', 'metronic');

    $js_scripts[] =   array('path' =>  base_path() . $path_to_theme . '/assets/global/plugins/jquery-1.11.0.min.js', 'options' => array('scope' => 'header', 'type' => 'file'));
    $js_scripts[] =   array('path' =>  base_path() . $path_to_theme . '/assets/global/plugins/jquery-migrate-1.2.1.min.js', 'options' => array('scope' => 'header', 'type' => 'file'));

    $js_scripts = array_merge($js_scripts, _default_js_list());
    $js_scripts = array_merge($js_scripts, _plugin_js_list($type));
}

