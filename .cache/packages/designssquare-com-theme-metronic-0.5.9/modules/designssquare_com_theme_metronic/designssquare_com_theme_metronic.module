    $desposable_elements[] = array('id' => '#site_statistics', 'remove' => 'content');
    $desposable_elements[] = array('id' => 'canvas', 'remove' => 'content');
    $desposable_elements[] = array('id' => 'canvas', 'remove' => 'element');
    $desposable_elements[] = array('id' => 'script', 'remove' => 'content');
    $desposable_elements[] = array('id' => 'script', 'remove' => 'element');
