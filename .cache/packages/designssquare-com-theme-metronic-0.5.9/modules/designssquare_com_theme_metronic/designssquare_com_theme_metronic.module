function designssquare_com_theme_metronic_ckeditor_css_alter(&$css_scripts, $vars)
{
    $type = (isset($vars['node']) && isset($vars['node']->type)) ? $vars['node']->type : 'default';
    $css_scripts = array_merge($css_scripts, array_map('_strip_path', _default_css_list($type)));
}

function designssquare_com_theme_metronic_ckeditor_body_class_alter(&$body_classes, $vars)
{
