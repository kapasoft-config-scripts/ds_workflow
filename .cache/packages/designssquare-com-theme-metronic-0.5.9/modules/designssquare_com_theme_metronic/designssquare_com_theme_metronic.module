function _default_css_list($plugin_type)
{
    $css_list = array();
     base_path() . $path_to_theme = drupal_get_path('theme', 'metronic');
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/drupal/custom_metronic.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME + 4));
//    drupal_add_css(drupal_get_path('theme', 'metronic') . '/assets/drupal/custom_metronic.css', array('type' => 'file', 'weight' => CSS_THEME + 4));

    //BEGIN GLOBAL MANDATORY STYLES -->
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/drupal/metronic_fonts.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/global/plugins/font-awesome/css/font-awesome.min.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/global/plugins/simple-line-icons/simple-line-icons.min.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/global/plugins/bootstrap/css/bootstrap.min.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/global/plugins/uniform/css/uniform.default.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));
//<!-- END GLOBAL MANDATORY STYLES -->

    array_merge($css_list, _plugin_css_list($plugin_type));

//<!-- BEGIN THEME STYLES -->
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/global/css/components.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/global/css/plugins.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/admin/layout/css/layout.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));

    //determine color
    module_load_include('inc', 'designssquare_lib_template_api', 'includes/template_api');
    $theme_tapi = _theme_settings_tapi();
    $proposed_css = drupal_get_path('theme', 'metronic') . '/assets/admin/layout/css/themes/' . $theme_tapi['color']['value'] . '.css';
    if (file_exists($proposed_css)) {
        $final_css = $proposed_css;
    } else {
        $final_css = drupal_get_path('theme', 'metronic') . '/assets/admin/layout/css/themes/default.css';
    }
    $css_list[] = array('path' => $final_css, 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));
    $css_list[] =   array('path' =>  base_path() . $path_to_theme . '/assets/admin/layout/css/custom.css', 'options' => array('type' => 'file', 'weight' => CSS_THEME, 'preprocess' => FALSE));
    return $css_list;
}

